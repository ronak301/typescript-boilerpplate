    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.152",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
