  "scripts": {
    "start": "PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "serve": "serve -l 9090 -c ./serve.json",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "jest:run": "react-scripts test --watchAll=false --env=jsdom-fourteen",
    "jest:watchAll": "react-scripts test --env=jsdom-fourteen",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:start": "start-server-and-test start http://localhost:8080 cypress:open",
    "cypress:ci": "BROWSER=none start-server-and-test start http://localhost:8080 cypress:run",
    "prettier:report": "prettier '**/*.{js,tsx,ts}' --check",
    "prettier:fix": "prettier '**/*.{js,tsx,ts}' --write",
    "eslint:report": "eslint '**/*.{js,tsx,ts}' --quiet",
    "eslint:fix": "eslint '**/*.{js,tsx,ts}' --fix"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.5",
    "@testing-library/jest-dom": "^5.11.4",
